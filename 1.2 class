class Parent():

    def __init__(self):
        print("Class 'Parent' is initation")

    def function_parent_1(self):
        print("Parent class")

    def function_parent_2(self):
        print("Parent class again")

class Child(Parent):

    def _init_(self):
        print("Class 'Child' is initation")

    def function_child(self):
        print("Child class")

obj_1 = Child()
obj_2 = Child()

obj_1.function_parent_1()
obj_2.function_parent_2()
