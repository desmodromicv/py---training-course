#!/usr/bin/env python3

def convert():
    input_1 = input("Give me a complex number: ")
    print('Your number is instance {}.\n'.format(type(input_1).__name__))

    print('Convertion your number...')
    input_1 = float(input_1)

    print('Your number is instance {}.\n'.format(type(input_1).__name__))
    print('Your number after convertion {}.'.format(input_1))

object_input = convert()
